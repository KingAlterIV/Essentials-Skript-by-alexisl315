					send "{@Essentials-use-spawn-use-teleport-use-spawnpoint-use-playeronline-use-playermessage-use-byconsole-use}" to arg 1
					stop
			if arg 1 is offline:
				send "{@Essentials-use-spawn-use-teleport-use-spawnpoint-use-playeroffline-use}" to console
				stop

