#  |-> PLUGIN CODE:

command /give [<text>] [<number>]:
	executable by: player and console
	aliases: essentials:give
	description: &fEssentials command /give !
	trigger:
		if sender is a player:
			if arg 1 is not set:
				send "{@Essentials-use-give-use-noneargument1-use}" to player
				stop
			if arg 1 is set:
				set {Essentials.use.give.use.itemtype.use.%player%.use} to arg 1
				replace all "_" with " " in {Essentials.use.give.use.itemtype.use.%player%.use}
				set {Essentials.use.give.use.itemtype.use.%player%.use} to "%{Essentials.use.give.use.itemtype.use.%player%.use}%" parsed as item
				wait 1 tick
				if {Essentials.use.give.use.itemtype.use.%player%.use} is an item:
					give {Essentials.use.give.use.itemtype.use.%player%.use} to player
					send "{@Essentials-use-give-use}" to player
					stop
				else:
					send "{@Essentials-use-give-use-noitemtype-use}" to player
					stop
		if sender is console:
			send "{@Essentials-use-console-use-nocommand-use}" to console
			stop

on command "give" or "essentials:give":
	if sender is a player:
		if player has permission "essentials.*" or "essentials.give":
			stop
		if player doesn't have permission "essentials.*" or "essentials.give":
			cancel event
			send "{@Essentials-use-player-use-nopermission-use}" to player
			stop

command /servers [<text>]:
	executable by: player and console
	aliases: essentials:servers
	description: &fEssentials command /servers !
	trigger:
		if sender is a player:
			if {Essentials.use.server.use::*} is set:
				send "{@Essentials-use-servers-use}" to player
				stop
			if {Essentials.use.server.use::*} is not set:
				send "{@Essentials-use-servers-use-noserver-use}" to player
				stop
		if sender is console:
			if {Essentials.use.server.use::*} is set:
				send "{@Essentials-use-servers-use}" to console
				stop
			if {Essentials.use.server.use::*} is not set:
				send "{@Essentials-use-servers-use-noserver-use}" to console
				stop

on command "servers" or "essentials:servers":
	if sender is a player:
		if player has permission "essentials.*" or "essentials.servers":
			stop
		if player doesn't have permission "essentials.*" or "essentials.servers":
			cancel event
			send "{@Essentials-use-player-use-nopermission-use}" to player
			stop

command /serverinfo [<text>]:
	executable by: player and console
	aliases: essentials:serverinfo
	description: &fEssentials command /serverinfo !
	trigger:
		if sender is a player:
			if arg 1 is not set:
				send "{@Essentials-use-serverinfo-use-noneargument1-use}" to player
				stop
			if arg 1 is set:
				if player has permission "essentials.*" or "essentials.serverinfo.*" or "essentials.serverinfo.%arg 1%":
					if {Essentials.use.server.use::%arg 1%} is not set:
						send "{@Essentials-use-serverinfo-use-noserver-use}" to player
						stop
					if {Essentials.use.server.use::%arg 1%} is set:
						send "{@Essentials-use-serverinfo-use}" to player
						stop
				if player doesn't have permission "essentials.*" or "essentials.serverinfo.*" or "essentials.serverinfo.%arg 1%":
					send "{@Essentials-use-player-use-nopermission-use}" to player
					stop
		if sender is console:
			send "{@Essentials-use-console-use-nocommand-use}" to console
			stop

on command "serverinfo" or "essentials:serverinfo":
	if sender is a player:
