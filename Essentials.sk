on break:
	if {@Essentials-use-log-use-blockbreak-use-config-use} is true:
		log "{@Essentials-use-log-use-blockbreak-use}" to "Essentials/%player%/Block-Break.log"
		stop
	if {@Essentials-use-log-use-blockbreak-use-config-use} is false:
		stop

on drop:
	if {@Essentials-use-log-use-drop-use-config-use} is true:
		log "{@Essentials-use-log-use-drop-use}" to "Essentials/%player%/Drop.log"
		stop
	if {@Essentials-use-log-use-drop-use-config-use} is false:
		stop

on death:
	if victim is a player:
		if {@Essentials-use-log-use-deathentity-use-config-use} is true:
			log "{@Essentials-use-log-use-deathentity-use}" to "Essentials/%player%/Death.log"
			stop
		if {@Essentials-use-log-use-deathentity-use-config-use} is false:
			stop

on command:
	if sender is a player:
		if {@Essentials-use-log-use-command-use-config-use} is true:
			log "{@Essentials-use-log-use-command-use}" to "Essentials/%player%/Command.log"
			stop
		if {@Essentials-use-log-use-command-use-config-use} is false:
			stop

on swap hand item:
	if {@Essentials-use-secondhand-use-switch-use-config-use} is true:
		stop
	if {@Essentials-use-secondhand-use-switch-use-config-use} is false:
		cancel event
		stop