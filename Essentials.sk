command /spawn [<offline player>] [<text>]:
	executable by: player and console
	aliases: essentials:spawn
	description: &fEssentials command /spawn !
	trigger:
		if sender is a player:
			if arg 1 is not set:
				if {Essentials.use.spawnpoint.use} is not set:
					send "{@Essentials-use-spawn-use-spawnpoint-use-noset-use}" to player
					stop
				if {Essentials.use.spawnpoint.use} is set:
					if {@Essentials-use-spawn-use-cooldown-use} is true:
						if player has permission "essentials.*" or "essentials.spawn.bypass":
							set {Essentials.use.backpoint.use.%player%.use} to location of player
							wait 1 tick
							teleport player to {Essentials.use.spawnpoint.use}
							wait 1 tick
							send "{@Essentials-use-spawn-use-teleport-use-spawnpoint-use}" to player
							stop
						if player doesn't have permission "essentials.*" or "essentials.spawn.bypass":
							if {Essentials.use.spawn.use.teleportation.use.already.use.%player%.use} is not set:
								set {Essentials.use.spawn.use.teleportation.use.already.use.%player%.use} to true
								send "{@Essentials-use-spawn-use-teleport-use-spawnpoint-use-cooldown-use}" to player
								wait {@Essentials-use-spawn-use-teleport-use-cooldown-use} seconds
								set {Essentials.use.backpoint.use.%player%.use} to location of player
								wait 1 tick
								teleport player to {Essentials.use.spawnpoint.use} 
								wait 1 tick
								delete {Essentials.use.spawn.use.teleportation.use.already.use.%player%.use}
								send "{@Essentials-use-spawn-use-teleport-use-spawnpoint-use}" to player
								stop
							if {Essentials.use.spawn.use.teleportation.use.already.use.%player%.use} is set:
								send "{@Essentials-use-spawn-use-teleport-use-spawnpoint-use-already-use}" to player
								stop
					if {@Essentials-use-spawn-use-cooldown-use} is false:
						set {Essentials.use.backpoint.use.%player%.use} to location of player
						wait 1 tick
						teleport player to {Essentials.use.spawnpoint.use}
						wait 1 tick
						send "{@Essentials-use-spawn-use-teleport-use-spawnpoint-use}" to player
						stop
			if arg 1 is set:
				if player has permission "essentials.*" or "essentials.spawn.%arg 1%" or "essentials.spawn.*":
					if {Essentials.use.spawnpoint.use} is not set:
						send "{@Essentials-use-spawn-use-spawnpoint-use-noset-use}" to player
						stop
					if {Essentials.use.spawnpoint.use} is set:
						if arg 1 is online:
							if arg 1 is "%player%":
								send "{@Essentials-use-spawn-use-teleport-use-spawnpoint-use-playerme-use}" to player
								stop
							if arg 1 is not "%player%":
								teleport arg 1 to {Essentials.use.spawnpoint.use}
								wait 1 tick
								send "{@Essentials-use-spawn-use-teleport-use-spawnpoint-use-playeronline-use}" to player
								send "{@Essentials-use-spawn-use-teleport-use-spawnpoint-use-playeronline-use-playermessage-use-byplayer-use}" to arg 1
								stop
						if arg 1 is offline:
							send "{@Essentials-use-spawn-use-teleport-use-spawnpoint-use-playeroffline-use}" to player
							stop
				if player doesn't have permission "essentials.*" or "essentials.spawn.%arg 1%" or "essentials.spawn.*":
					send "{@Essentials-use-player-use-nopermission-use}" to player
					stop
		if sender is console:
			if arg 1 is not set:
				send "{@Essentials-use-spawn-use-teleport-use-spawnpoint-use-noneargument1-use-byconsole-use}" to console
				stop
			if arg 1 is online:
				if {Essentials.use.spawnpoint.use} is not set:
					send "{@Essentials-use-spawn-use-spawnpoint-use-noset-use}" to console
					stop
