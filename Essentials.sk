		stop

on command:
	if {Essentials.use.randomteleport.use.change.use.gamemode.use.%player%.use} is set:
		cancel event
		wait {@Essentials-use-randomteleport-use-teleport-use-changegamemode-use-creative-use} seconds
		set gamemode of player to survival
		delete {Essentials.use.fly.use.%player%.use}
		delete {Essentials.use.randomteleport.use.change.use.gamemode.use.%player%.use}
		delete {Essentials.use.randomteleport.use.teleport.use.%player%.use}
		set {Essentials.use.randomteleport.use.teleport.use.%player%.use.antidamage.use} to true
		stop
	if {Essentials.use.randomteleport.use.change.use.gamemode.use.%player%.use} is not set:
		stop

on any movement:
	if {Essentials.use.randomteleport.use.change.use.gamemode.use.%player%.use} is set:
		cancel event
		wait {@Essentials-use-randomteleport-use-teleport-use-changegamemode-use-creative-use} seconds
		set gamemode of player to survival
		delete {Essentials.use.fly.use.%player%.use}
		delete {Essentials.use.randomteleport.use.change.use.gamemode.use.%player%.use}
		delete {Essentials.use.randomteleport.use.teleport.use.%player%.use}
		set {Essentials.use.randomteleport.use.teleport.use.%player%.use.antidamage.use} to true
		stop
	if {Essentials.use.randomteleport.use.change.use.gamemode.use.%player%.use} is not set:
		stop

on damage:
	if victim is a player:
		if {Essentials.use.randomteleport.use.teleport.use.%victim%.use.antidamage.use} is set:
			cancel event
			wait {@Essentials-use-randomteleport-use-teleport-use-changegamemode-use-creative-use} seconds
			set gamemode of victim to survival
			delete {Essentials.use.fly.use.%victim%.use}
			delete {Essentials.use.randomteleport.use.change.use.gamemode.use.%victim%.use}
			delete {Essentials.use.randomteleport.use.teleport.use.%victim%.use}
			wait {@Essentials-use-randomteleport-use-teleport-use-cooldown-use} seconds
			delete {Essentials.use.randomteleport.use.teleport.use.%victim%.use.antidamage.use}
			stop
		if {Essentials.use.randomteleport.use.teleport.use.%victim%.use.antidamage.use} is not set:
			stop

on join:
	delete {Essentials.use.randomteleport.use.change.use.gamemode.use.%player%.use}
	delete {Essentials.use.randomteleport.use.teleport.use.%player%.use}
	stop

on quit:
	delete {Essentials.use.unlimited.use.%player%.use}
	stop

on place:
	if {Essentials.use.unlimited.use.%player%.use} is set:
		give 0 of event-block to the player
		stop
	if {Essentials.use.unlimited.use.%player%.use} is not set:
		stop

on join:
	if {Essentials.use.banip.use.%player%.use} is set:
		wait 1 tick
		kick player due to "{@Essentials-use-banip-use-kick-use-playerconnect-use-message-use}"
		stop
	if {Essentials.use.banip.use.%player%.use} is not set:
		stop

on join:
	if {Essentials.use.ban.use.%player%.use} is set:
		wait 1 tick
		kick player due to "{@Essentials-use-ban-use-kick-use-playerconnect-use-message-use}"
		stop
	if {Essentials.use.ban.use.%player%.use} is not set:
		stop

on join:
	if {Essentials.use.tempban.use.%player%.use} is set:
		if difference between now and {Essentials.use.tempban.use.%player%.use.times.use.now.use} is bigger than {Essentials.use.tempban.use.%player%.use.times.use}:
			delete {Essentials.use.banip.use.%player%.use}
			delete {Essentials.use.banip.use.%player%.use.reason.use}
			delete {Essentials.use.banip.use.list.use::%player%}
			delete {Essentials.use.tempban.use.%player%.use}
			delete {Essentials.use.tempban.use.%player%.use.reason.use}
			delete {Essentials.use.tempban.use.%player%.use.times.use}
			delete {Essentials.use.tempban.use.%player%.use.times.use.now.use}
			delete {Essentials.use.tempban.use.list.use::%player%}
			delete {Essentials.use.ban.use.%player%.use}
			delete {Essentials.use.ban.use.%player%.use.reason.use}
			delete {Essentials.use.ban.use.list.use::%player%}
			stop
		if difference between now and {Essentials.use.tempban.use.%player%.use.times.use.now.use} is smaller than {Essentials.use.tempban.use.%player%.use.times.use}:
			wait 1 tick
			kick player due to "{@Essentials-use-tempban-use-kick-use-playerconnect-use-message-use}"
			stop
	if {Essentials.use.tempban.use.%player%.use} is not set:
		stop

on quit:
	set the player's walk speed to 0.2
	set the player's fly speed to 0.1
	stop

every 1 second:
	loop all players:
		if {Essentials.use.mute.use.seconds.use.%loop-player%.use} is more than 0:
			remove 1 from {Essentials.use.mute.use.seconds.use.%loop-player%.use}
			stop
		else:
			delete {Essentials.use.mute.use.seconds.use.%loop-player%.use}
			delete {Essentials.use.mute.use.list.use::%loop-player%}
			stop

on join:
	if player's gamemode is survival or adventure:
		delete {Essentials.use.god.use.%player%.use}
		stop
	if player's gamemode is creative or spectator:
		set {Essentials.use.god.use.%player%.use} to true
		stop

on damage:
	if {Essentials.use.god.use.%victim%.use} is true:
		cancel event
		stop
	if {Essentials.use.god.use.%victim%.use} is false:
		stop

on world init:
	if {@Essentials-use-world-use-create-use-config-use} is true:
		send "{@Essentials-use-world-use-create-use-message-use}" to console
		stop
	if {@Essentials-use-world-use-create-use-config-use} is false:
		stop

on connect:
	set {Essentials.use.ip.use.%player%.use} to ip of player
	stop

on every 1 tick in {@Essentials-use-banip-use-checkip-use-worldname-use}:
	loop all players:
		if {Essentials.use.banip.use.%{Essentials.use.ip.use.%loop-player%.use}%.use.ip.use} is set:
			kick loop-player due to "{@Essentials-use-banip-use-kick-use-checkip-use-playerconnect-use-message-use}"
			stop
		if {Essentials.use.banip.use.%{Essentials.use.ip.use.%loop-player%.use}%.use.ip.use} is not set:
			stop

on chat:
	if {@Essentials-use-log-use-chat-use-config-use} is true:
		log "{@Essentials-use-log-use-chat-use}" to "Essentials/%player%/Chat.log"
		stop
	if {@Essentials-use-log-use-chat-use-config-use} is false:
		stop

on build:
	if {@Essentials-use-log-use-blockbuild-use-config-use} is true:
		log "{@Essentials-use-log-use-blockbuild-use}" to "Essentials/%player%/Block-Build.log"
		stop
	if {@Essentials-use-log-use-blockbuild-use-config-use} is false:
